{
  "name": "typescript-project",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/webbestmaster/typescript-project.git"
  },
  "bugs": {
    "url": "https://github.com/webbestmaster/typescript-project/issues"
  },
  "homepage": "https://github.com/webbestmaster/typescript-project#readme",
  "license": "MIT",
  "author": "",
  "version": "0.0.0",
  "description": "",
  "keywords": [
    "typescript-project",
    "empty"
  ],
  "main": "dist/index.js",
  "types": "dist/library.d.ts",
  "files": [
    "/dist"
  ],
  "scripts": {
    "start": "cross-env NODE_ENV=development SIDE=front webpack-cli serve --progress",
    "start:transpile-only": "cross-env NODE_ENV=development SIDE=front TS_TRANSPILE_ONLY=true webpack-cli serve --progress",
    "tsc-watch": "tsc -p ./tsconfig.json --watch",
    "server:start": "node ./dist-server/dist/index.js",
    "server:build": "cross-env NODE_ENV=production SIDE=back IS_SERVER_PROD_BUILD=true NODE_OPTIONS=--max-old-space-size=1536 webpack --progress",
    "server:watch:build": "npm run build && cross-env NODE_ENV=production SIDE=back webpack --progress --watch",
    "server:nodemon": "nodemon ./dist-server/dist/index.js",
    "build": "cross-env NODE_ENV=production SIDE=front NODE_OPTIONS=--max-old-space-size=1536 webpack --progress",
    "build-library": "cross-env NODE_ENV=production SIDE=front IS_BUILD_LIBRARY=true NODE_OPTIONS=--max-old-space-size=1536 webpack --progress && npm run tsc-library",
    "check": "npm run ts-coverage && npm run tsc && npm run eslint && npm run stylelint",
    "check:fix": "npm run prettier && npm run ts-coverage && npm run tsc && npm run eslint:fix && npm run stylelint:fix",
    "eslint": "eslint .",
    "eslint:fix": "eslint . --fix",
    "stylelint": "stylelint ./**.scss",
    "stylelint:fix": "stylelint ./**.scss --fix",
    "prettier": "prettier --write \"./**/*.(js|ts|tsx)\" --loglevel warn",
    "tsc": "tsc -p ./tsconfig.json",
    "tsc-library": "tsc -p ./tsconfig.library.json && npm run tsc-library-css",
    "tsc-library-css": "cross-env NODE_ENV=production SIDE=front IS_BUILD_LIBRARY=true node ./webpack/util/library/css.js",
    "ts-coverage:front": "typescript-coverage-report --threshold=100 --strict=true --project ./www/",
    "ts-coverage:server": "typescript-coverage-report --threshold=100 --strict=true --project ./server/",
    "ts-coverage": "npm run ts-coverage:front && npm run ts-coverage:server",
    "test:e2e": "jest --config='test-e2e/config/jest.config.js' ./test-e2e/ --runInBand --silent --passWithNoTests",
    "test:unit": "jest --config='test-unit/config/jest.config.js' ./www/ --coverage --silent --passWithNoTests",
    "pre-commit-hook": "npm run check",
    "_postinstall": "cp ./git-hooks/pre-commit ./.git/hooks"
  },
  "dependencies": {
    "@types/react": "^17.0.37",
    "@types/react-dom": "^17.0.11",
    "@types/sqlite3": "^3.1.7",
    "ajv": "^8.8.2",
    "fastify": "^3.24.0",
    "fastify-static": "^4.5.0",
    "markdown-pro": "^1.0.22",
    "react": "^17.0.2",
    "react-audio-player-pro": "^1.1.19",
    "react-dom": "^17.0.2",
    "react-localization-library": "^0.0.18",
    "react-router": "^6.0.2",
    "react-router-dom": "^6.0.2",
    "react-system-hook": "^0.0.14",
    "sqlite3": "^5.0.2"
  },
  "devDependencies": {
    "@babel/core": "^7.16.0",
    "@babel/plugin-transform-strict-mode": "^7.16.0",
    "@svgr/webpack": "^5.5.0",
    "@testing-library/jest-dom": "^5.15.1",
    "@testing-library/react": "^12.1.2",
    "@types/jest": "^27.0.3",
    "@types/puppeteer": "^5.4.4",
    "@typescript-eslint/eslint-plugin": "^5.4.0",
    "@typescript-eslint/parser": "^5.4.0",
    "babel-eslint": "^10.1.0",
    "babel-loader": "^8.2.3",
    "circular-dependency-plugin": "^5.2.2",
    "clean-webpack-plugin": "^4.0.0",
    "copy-webpack-plugin": "^10.0.0",
    "cross-env": "^7.0.3",
    "css-loader": "^6.5.1",
    "css-modules-typescript-loader": "^4.0.1",
    "duplicate-package-checker-webpack-plugin": "^3.0.0",
    "eslint": "^8.3.0",
    "eslint-import-resolver-typescript": "^2.5.0",
    "eslint-plugin-babel": "^5.3.1",
    "eslint-plugin-filenames": "^1.3.2",
    "eslint-plugin-import": "^2.25.3",
    "eslint-plugin-jsx-a11y": "^6.5.1",
    "eslint-plugin-no-loops": "^0.3.0",
    "eslint-plugin-no-use-extend-native": "^0.5.0",
    "eslint-plugin-optimize-regex": "^1.2.1",
    "eslint-plugin-promise": "^5.1.1",
    "eslint-plugin-react": "^7.27.1",
    "eslint-plugin-react-hooks": "^4.3.0",
    "eslint-plugin-sonarjs": "^0.10.0",
    "eslint-plugin-typescript-sort-keys": "^2.1.0",
    "eslint-plugin-unicorn": "^39.0.0",
    "file-loader": "^6.2.0",
    "html-webpack-plugin": "^5.5.0",
    "jest": "^27.3.1",
    "jest-transform-stub": "^2.0.0",
    "mini-css-extract-plugin": "^2.4.5",
    "nodemon": "^2.0.15",
    "optimize-css-assets-webpack-plugin": "^6.0.1",
    "postcss": "^8.4.3",
    "postcss-scss": "^4.0.2",
    "prettier": "^2.5.0",
    "puppeteer": "^11.0.0",
    "raw-loader": "^4.0.2",
    "sass": "^1.43.5",
    "sass-loader": "^12.3.0",
    "script-ext-html-webpack-plugin": "^2.1.5",
    "style-loader": "^3.3.1",
    "stylelint": "^14.1.0",
    "stylelint-config-sass-guidelines": "^9.0.1",
    "stylelint-config-standard": "^24.0.0",
    "stylelint-scss": "^4.0.0",
    "terser-webpack-plugin": "^5.2.5",
    "ts-jest": "^27.0.7",
    "ts-loader": "^9.2.6",
    "typescript": "^4.5.2",
    "typescript-coverage-report": "^0.6.1",
    "webpack": "^5.64.4",
    "webpack-bundle-analyzer": "^4.5.0",
    "webpack-cli": "^4.9.1",
    "webpack-dev-server": "^4.6.0",
    "webpack-node-externals": "^3.0.0"
  }
}
